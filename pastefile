
        var parameter = new SqlParameter("@MA", "M");

        var sources = await _context.Sources
            .FromSqlRaw("EXEC GetSources @MA", parameter) // Pass parameter
            .ToListAsync();

        return sources.Select(s => new SelectListItem
        {
            Value = s.REF_ID.ToString(),  // Use REF_ID for value
            Text = s.REF_DESC             // Use REF_DESC for text
        }).ToList();
